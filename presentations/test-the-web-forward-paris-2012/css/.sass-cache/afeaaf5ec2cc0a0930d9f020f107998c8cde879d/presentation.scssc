}

html {
	background-image: -webkit-gradient(
		radial, 
		0% 0%, 0, 
		50% 50%, 1000, 
		from(rgba(50,50,50,0.7)), 
		to(rgba(50,50,50,1))
	);

	background-image: -moz-radial-gradient(
		50% 50% 90deg,
		rgba(50,50,50,1.0) 0%, 
		rgba(50,50,50,1.0) 100%

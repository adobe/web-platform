/*********************************************
 * GLOBAL STYLES
 *********************************************/

body {
    counter-reset: slide;
}

html, body {
	padding: 0;
	margin: 0;
	overflow: hidden;
	font-weight: 100;
	font-size: 36px;
	
	background: none;
	color: #f0f0f0;
	text-shadow: 2px 2px 2px rgba(0, 0, 0, 0.5);
	

}

.filter-shorthands.interactive li {
    cursor: pointer;
}

.blend-mode, .compositing-rule {
    float: left;
    width: 300px;
}


.blend-mode img, .compositing-rule img {
    height: 150px;
}

.blend-mode, .compositing-rule {
    font-size: 0.7em;
}

.filter-image {
    width: 120px;
    height: 90px;
    background-image: url(../assets/images/slides/iStockPhoto/iStock_000016862713XXSmall.jpg);
    background-size: 120px 90px;
    border: none;
}

.filter-shorthand.code {
    font-weight: bold;
    font-size: 0.4em;
    margin-top: 0.25em;
}

.filter-sample .frame {
    border: 0.25em solid white;
    box-shadow: 0px 0px 6px rgba(0, 0, 0, 0.25);    
}

.filter-sample {
    float: left;
    margin-left: 1em;
}

.filter-sample:first-child {
    margin-left: 0em;
}

#the-end h3 {
    line-height: 1.2;
}

.reveal .icon-bar {
    margin-left: 2em;
    margin-top: 0.1em;
    font-size: 0.4em;
    position: absolute;
    z-index: 100;
    -webkit-blend-mode: normal;
    padding-left: 5px;
    padding-top: 8px;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    background: rgba(0, 0, 0, 0.1);
    -webkit-transform: translate(0px, -45px);
    transition: -webkit-transform 0.2s ease-in;
}

.reveal .icon-bar:hover {
    -webkit-transform: translate(0px, 0px);
}

.reveal .icon-bar img {
    vertical-align: middle;
}

.reveal .icon-bar a {
    text-decoration: none;
    color: rgba(74, 80, 86, 0.5);
    font-weight: 300;
}

.reveal .icon-bar a:hover {
    color: white;
    text-shadow: 0px -1px 2px rgba(255, 255, 255, 0.5);
    text-decoration: underline;
}

.reveal .icon-bar a span {
    padding-left: 0.25em;
    padding-right: 1em;
    color: white;
    text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.25);
    font-weight: normal;
}

.reveal .icon-list li {
    float: left;
    display: block;
    width: 3em;
    font-weight: 300;
    text-align: center;
}

.reveal .icon-list .icon {
    width: 50px;
    height: 50px;
    display: block;
    background: rgba(255, 255, 255, 0.5);
    border: 5px solid white;
    box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.25);
    margin-left: auto;
    margin-right: auto;
}

.reveal .icon-list a {
    font-size: 0.5em;
    font-weight: 700;
}

.reveal .icon-list .icon-attrib {
    display: block;
    margin-left: auto;
    margin-right: auto;
    max-width: 80px;
    font-size: 0.4em;
    line-height: 1.2em;
}

.reveal img.thumb-attrib {
    vertical-align: -30%;
    border: 5px solid white;
    box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.25);
    margin-left: 0.2em;
    margin-right: 0.3em;
    margin-top: 0em;
}

.reveal .texture-samples li {
    float: left;
    display: block;
    width: 3em;
    margin-bottom: 0em;
}

.texture-samples li span {
    display: block;
    width: 50px;
    height: 50px;
    margin-left: auto;
    margin-right: auto;
    box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.25);
    border: 5px solid white;
    margin-bottom: 0.25em;
}

.texture-samples li a:not(.image) {
    font-size: 0.5em;
    line-height: 1.1em;
    font-weight: 300;
    display: block;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
}

#presentation-url {
    font-size: 2em;
    margin-top: 2em;
}

.palette {
    display: none;
    position: absolute;
    right: 0px;
    top: 30px;
    background: rgba(0, 0, 0, 0.2);
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    padding: 5px;
}

.palette .swatch {
    display: block;
    width: 20px;
    height: 20px;
    border: 1px solid rgba(0, 0, 0, 0.2);
    margin-top: 5px;
}

.palette .swatch:first-child {
    margin-top: 0;
}

#hashtag {
    position: absolute;
    left: 10px;
    bottom: 20px;
    color: rgba(255, 255, 255, 0.5);
    font-size: 0.5em;
    font-weight: bold;
}

#postit {
    width: 200px;
    height: 230px;
    padding: 1em;
    border: 1px solid #C9C04B;
    box-shadow: 0.2em 0.2em 0.4em rgba(0, 0, 0, 0.5);
    border-radius: 0px 0px 0em 0;
    font-family: "caflisch-script-pro";
    text-shadow: none;
    color: $highlight-color-3;
    font-size: 50px;
    font-weight: 700;
    margin-left: auto;
    margin-right: auto;
    background: #fefcea; /* Old browsers */
    background: linear-gradient(-45deg,  #fefcea 0%,#f1da36 100%); /* W3C */
    -webkit-transform: rotateZ(-2deg);
    transition: border-radius 0.2s linear;
    text-shadow: 0px 0px 4px rgba(0, 0, 0, 0.2);
}

#postit.curled {
    border-radius: 0px 0px 3em/1em 0;
    
}

#postit.curled:hover {
    border-radius: 0px 0px 0em 0;
}


#web-of-boxes .box-style {
    display:inline-block;
    width:100px;
    height:100px;
    border: 0.5em solid $highlight-color-3;
    background: $highlight-color-4;
    margin: 1em;
    box-shadow: 0.2em 0.2em 0.2em rgba(0, 0, 0, 0.25);
    transition: border 0.2s linear;
}

#web-of-boxes .box-style:nth-child(2) {
    border: 0.5em solid $highlight-color-2;
}

#web-of-boxes .box-style:hover {
    border: 0.5em solid $highlight-color-1;
}

#css-shaders-studio {
    -webkit-transform: rotateZ(-5deg);
}

.reveal .talk.list > li{
    list-style-position: outside;
    list-style-type: none;
    margin-bottom: 1em;
}

.reveal .talk.list > li > * {
    line-height: 1em;
}

.reveal .talk.list em {
    text-shadow: none;
    font-style: normal;
    font-weight: bold;
}

.reveal .talk.time {
    display: block;
    font-size: 0.5em;
}

.reveal .talk.speaker:before {
    content: "Speaker: ";
}

.reveal .talk.speaker {
    display: block;
    font-size: 0.5em;
    font-weight: bold;
    opacity: 0.5;
}

.reveal .talk.title {
    display: block;
    font-weight: 700;
}

.reveal section img.brazil-flag.step-0 {
    animation-name: none;
    filter: grayscale(0);
    width: 80%;
    margin-top: 0;
}  

.reveal section img.brazil-flag.step-1 {
    -webkit-filter: custom(url('../wobble.vs') none, 40 40 content-box, amplitude 60, amount 0.4, angle 20, flag_texture texture(url(../assets/images/slides/grunge-texture.jpg)));
    -webkit-filter-margin: 0px 0px 0px 0px;
    width: 80%;
    margin-top: 0;
}

.reveal section img.brazil-flag.step-2 {
    -webkit-filter: custom(url('../wobble.vs') url('../color-swipe.fs'), 40 40 content-box, amplitude 60, amount 0.4, angle 20, flag_texture texture(url(../assets/images/slides/grunge-texture.jpg)));
    -webkit-filter-margin: 0px 0px 0px 0px;
    width: 80%;
    margin-top: 0;
}

.reveal section img.brazil-flag.step-3 {
    -webkit-filter: custom(url('../wobble.vs') url('../color-swipe.fs'), 40 40 content-box, amplitude 60, amount 0.5, angle 20, flag_texture texture(url(../assets/images/slides/grunge-texture.jpg)));
    -webkit-filter-margin: 0px 0px 0px 0px;
    animation-name: flagmove;
    animation-duration: 7s;
    animation-iteration-count: infinite;
    animation-direction: normal;
    animation-timing-function: ease-in-out;
    width: 80%;
    margin-top: 0;
}

@keyframes flagmove {
    0% {
        -webkit-filter: custom(url('../wobble.vs') url('../color-swipe.fs'), 40 40 content-box, amplitude 60, amount 0.3, angle 20, flag_texture texture(url(../assets/images/slides/grunge-texture.jpg)));
    }
    50% {
        -webkit-filter: custom(url('../wobble.vs') url('../color-swipe.fs'), 40 40 content-box, amplitude 60, amount 1, angle 20, flag_texture texture(url(../assets/images/slides/grunge-texture.jpg)));
    }
    100% {
        -webkit-filter: custom(url('../wobble.vs') url('../color-swipe.fs'), 40 40 content-box, amplitude 60, amount 0.3, angle 20, flag_texture texture(url(../assets/images/slides/grunge-texture.jpg)));
    }
}

.treasure .state-background {
    background-color: #404040;
    background: url(../assets/images/slides/treasure-map.jpg);
    background-size: cover;
    opacity: 1;
}

.grey-sandbag .state-background {
    background: url(../assets/images/slides/grey_sandbag.png);
    opacity: 1;
}

/* .google .state-background {
    background: black;
    opacity: 1;
}*/

.baby .state-background {
    background: url(../assets/images/slides/baby.png);
    background-size: cover;
    opacity: 1;
}

.rocket .state-background {
    background: url(../assets/images/slides/rocket.png);
    background-size: cover;
    opacity: 1;
}

.shocked .state-background {
    background: url(../assets/images/slides/iStockPhoto/iStock_000013141120Medium_gray.jpg);
    background-size: cover;
    opacity: 1;
}

.security .state-background {
    background: url(../assets/images/slides/iStockPhoto/iStock_000009667961Medium_gray.jpg);
    background-size: cover;
    opacity: 1;
}

.desert .state-background {
    background: url(../assets/images/slides/in-the-desert.jpg);
    background-size: cover;
    opacity: 1;
}

.dark .state-background {
    background: -webkit-radial-gradient(center, ellipse cover, rgba(30,87,153,0) 0%,rgba(0,0,0,0.5) 100%);
    opacity: 1;
}

.opening .state-background {
    background: #a0a0a0;
    background-size: cover;
    opacity: 1;
}

.opening .state-background svg {
    display: block;
}

.candy-store .state-background {
    background: url(../assets/images/slides/iStockPhoto/iStock_000016147591Medium.jpg);
    background-size: cover;
    opacity: 1;
    backgrond-position: bottom;
}

.fragment.what, .fragment.sorry, .fragment.drought, .fragment.css-security {
    font-size: 3em;
    line-height: 1.1em;
    color: white;
    font-weight: bold;
    font-family: 'source-sans-pro', 'SourceSansPro', serif;
    text-shadow: 0px -1px 1px rgba(0, 0, 0, 0.5) , 3px 3px 6px rgba(0, 0, 0, 0.5);
    position: absolute;
    top: 50%;
    margin-top: -1.5em;
    left: 0;
}

.fragment.css-security {
    text-align: left;
}

.sorry, .drought {
    left: auto;
    top: 50%;
    margin-top: -1.5em;
    line-height: 1em;
    
}

.candy-store .reveal h1.inverted {
    color: white;
    font-size: 3em;
    text-shadow: 3px 3px 6px rgba(0, 0, 0, 0.5);
}

.opening .reveal h1 {
    color: white;
    text-shadow: 0px -1px 0px rgba(0, 0, 0, 0.2), 3px 3px 6px rgba(0, 0, 0, 0.5);
    width: 100%;
}

.opening .reveal h1 span {
    display: block;
    font-size: 0.7em;
}

.reveal h1.last {
    margin-top: 2em;
}

.reveal h1 > a:not(.image) {
    color: white;
    text-shadow: 0px -1px 0px rgba(0, 0, 0, 0.2), 3px 3px 6px rgba(0, 0, 0, 0.5);
    font-size: 1.2em;
}


.reveal h1 > a:not(.image):hover {
    color: $highlight-color-1;
    text-shadow: 0px -1px 0px rgba(0, 0, 0, 0.2), 3px 3px 6px rgba(0, 0, 0, 0.5);
}

.reveal h2.stamp-available::before, 
.reveal h2.stamp-concept-demo::before, 
.reveal h2.stamp-public-prototype::before {
    width: 326px;
    position: absolute;
    top: 0px;
    right: 0px;
    -webkit-transform: scale(0.8) translate(100px, -50px) rotate(20deg);
    -webkit-blend-mode: multiply;
    opacity: 0.65;
}

.reveal h2.stamp-available::before {
    content: url(../assets/images/slides/stamp-available.svg);
}

.reveal h2.stamp-concept-demo::before {
    content: url(../assets/images/slides/stamp-concept-demo.svg);
    -webkit-transform: scale(0.8) translate(100px, -120px) rotate(20deg);
}

.reveal h2.stamp-public-prototype::before {
    content: url(../assets/images/slides/stamp-public-prototype.svg);
    -webkit-transform: scale(0.8) translate(50px, -70px) rotate(40deg);
}


/***********************************
 * Opening slide styling
 ***********************************/
#opening-svg text {
    fill: $dark-color;
}

#opening-svg text#subtitle {
    fill: white;
    text-shadow: 1px 1px 5px rgba(0, 0, 0, 0.5);
    font-weight: bold;
}


/***********************************
 * Shader content sampler styling
 ***********************************/
 
.reveal .shader-content-sample img {
    box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.2);
    margin-top: 0px;
    border: 1px solid white;
    position: relative;
 }
 
.reveal  .shader-content-sample {
    background: white;
    border: 10px solid white;
    width: 150px;
    margin-left: auto;
    margin-right: auto;
    box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.25);
 }
 
.reveal .shader-content-sample p.caption {
    color: $dark-color;
    font-size: 0.5em;
    font-weight: bold;
    margin: 0;
}

.reveal .demo-area .shader-content-sample {
    float: none;
}


.reveal .demo-area {
	display: box;
	box-align: center;
	width: 100%;
	height: 100%;
}

.reveal .demo-area > div {
	box-flex: 1;
}

.reveal .demo-area > div.panel-wrapper {
    box-flex: 0;
}


/***********************************
 * Control panel styling
 ***********************************/

.reveal .control-panel {
    width: 270px;
    border-radius: 8px;
    background: rgba(0, 0, 0, 0.75);
    border: 1px solid rgba(255, 255, 255, 0.3);
    box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.5);
    color: white;
    text-shadow: none;
    font-size: 16px;
    line-height: 1.2em;
    padding: 0.5em;
}

.reveal .control-panel input {
    width: 90%;
}

/***********************************
 * Shocked styling
 ***********************************/
.reveal .shocked {
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -3.5em;
}


/**************************************
 * Business cards layout
 **************************************/
 
.reveal .business-card {
    display: inline-block;
    width: 250px;
    height: 450px;
    border-radius: 10px;
    color: hsl(0, 0, 50%);
    font-size: 18px;
    border: 1px solid #c0c0c0;
    box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.2);
    text-align: left;
    line-height: 1.2em;
    padding: 1em;
    position: relative;
    background: white;
    margin: 1em;
}

.reveal .business-card.google {
    display: inline-block;
    width: 450px;
    height: 250px;
    border-radius: 10px;
    color: hsl(0, 0, 50%);
    font-size: 18px;
    border: 1px solid #c0c0c0;
    box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.2);
    text-align: left;
    line-height: 1.2em;
    padding: 1em;
    position: relative;
    background: white;
    margin: 1em;
    font-family: 'Open Sans';
}


.reveal .business-card:before {
    content: '';
    background: url(../assets/images/biz-logo.svg) no-repeat;    
    position: absolute;
    width: 30px;
    height: 38px;
    bottom: 1em;
    left: 1em;
}


.reveal .business-card.google:before {
    content: '';
    background: url(../assets/images/slides/google-logo.png) no-repeat;    
    position: absolute;
    width: 200px;
    height: 69px;
    top: 80px;
    left: 15px;
}


.reveal .business-card p {
    margin-bottom: 0px;
}

.reveal .business-card .name {
    text-transform: uppercase;
    font-weight: bold;
    font-size: 18px;
}

.reveal .business-card .title, .reveal .business-card .team {
    font-size: 14px;
}

.reveal .business-card.google .name, .reveal .business-card.google .title, .reveal .business-card.google .team,
    .reveal .business-card.google .contact-info{
    margin-left: 250px;
}

.reveal .business-card .contact-info {
    margin-top: 2em;
}


.reveal .business-card .contact-info a {
    color: hsl(0, 0, 50%);
    font-size: 14px;
    font-weight: normal;
    text-shadow: none;
    line-height: 1em;
}

.reveal .business-card li {
    list-style: none;
    margin-bottom: 0px;
}

.reveal .business-card .google-plus:before {
    content: "g+ ";
    font-size: 14px;
}

.reveal img.head-shot {
    display: inline-block;
    box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.4);
    border: 1px solid #e0e0e0;
    margin: 0;
    border: 10px solid white;
    filter: custom(url(../assets/shaders/flag.vs) url(../assets/shaders/shadows.fs), 20 20, phase 270.0, amplitude 80.0, txf rotateX(0deg));
}

.reveal #opening h1 {
    text-align: right;
}

.reveal #opening h1 > span {
    margin-top: 1em;
    display: block;
    opacity: 0.5;
    font-size: 0.5em;
    line-height: 1em;
    text-shadow: hsla(0, 0%, 0%, 0.9) 1px 1px 3px;
}

.reveal #opening h1 > span span {
    display: block;
    font-size: 0.8em;
    opacity: 1;
    font-weight: bold;
}

.reveal #author > a {
    display: block;
    text-align: right;
    font-size: 0.8;
    opacity: 0.8;
}

.reveal img.tool {
    position: absolute;
}

.reveal #tools img {
    box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.5);
}

/*************************************************
 * Agenda table slide
 *************************************************/
 
.reveal .agenda-table tr:nth-child(odd) {
    background: rgba(0, 0, 0, 0.1);
}

.reveal .agenda-table tr:first-child {
    font-weight: bold;
}


.reveal .agenda-table td:first-child {
    width: 150px;
}

.reveal .agenda-table td:nth-child(odd) {
    color: rgba(255, 255, 255, 0.5);
}


.reveal .progress {
	position: fixed;
	display: none;
	height: 10px;
	width: 100%;
	bottom: 0;
	left: 0;

	background: rgba(0,0,0,0.2);
}
	
	.reveal .progress span {
		display: block;
		/* From http://subtlepatterns.com/?p=934 */
		background: url(../assets/images/darkdenim3.png);
		height: 100%;
		width: 0px;

		transition: width 800ms cubic-bezier(0.260, 0.860, 0.440, 0.985);
	}


    /*********************************************
     * CONTROLS
     *********************************************/

.reveal .controls {
	display: none;

/*********************************************
 * GLOBAL STYLES
 *********************************************/

body {
    counter-reset: slide;
}

html, body {
	padding: 0;
	margin: 0;
	overflow: hidden;
	font-weight: 100;
	font-size: 36px;
	
	background: none;
	color: #f0f0f0;
	text-shadow: 2px 2px 2px rgba(0, 0, 0, 0.5);
	
	width: 100%;
	height: 100%;
}

html {
    /* From http://subtlepatterns.com/?p=922 */
    background-image: url(../assets/images/slides/groovepaper.png); 
    background-image: url(../assets/images/slides/exclusive_paper.png);
}

html {
	background-image: -webkit-gradient(
		radial, 
		0% 0%, 0, 
		50% 50%, 1000, 
		from(rgba(50,50,50,0.7)), 
		to(rgba(50,50,50,1))
	);

	background-image: -moz-radial-gradient(
		50% 50% 90deg,
		rgba(50,50,50,1.0) 0%, 
		rgba(50,50,50,1.0) 100%
	);
	
}

body {
    background-image: url(../assets/images/slides/background-dark.png);
}

body::after {
    content: url(../assets/images/logo.svg);

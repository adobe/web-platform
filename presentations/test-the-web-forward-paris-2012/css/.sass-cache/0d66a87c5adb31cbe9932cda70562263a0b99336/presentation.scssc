/*********************************************
 * VIEW FRAGMENTS
 *********************************************/

 .reveal .slides section .fragment.zoom {
    opacity: 1;
 	-webkit-transform: scale(0);
 }

 .reveal .slides section .fragment.zoom.visible {
    opacity: 1;
 	-webkit-transform: scale(1);
 }

 .reveal .slides section .fragment.zoom.rotate {
    opacity: 1;
 	-webkit-transform: rotateZ(125deg) scale(0);
 }

 .reveal .slides section .fragment.zoom.rotate.visible {
    opacity: 1;
 	-webkit-transform: rotateZ(0deg) scale(1);
 }

 .reveal .slides section .fragment.flip {
 	-webkit-transform: rotateY(90deg);
 }

 .reveal .slides section .fragment.flip.visible {
 	-webkit-transform: rotateY(0deg);
 }
 
 .reveal .slides section .fragment.flipx {
    opacity: 0;
 	-webkit-transform: rotateX(90deg);
 }

 .reveal .slides section .fragment.flipx.visible {
    opacity: 1;
 	-webkit-transform: rotateX(0deg);
 }
 
 .reveal .slides section .fragment.slide-in {
 	-webkit-transform: translateX(1000px);
 }

 .reveal .slides section .fragment.slide-in.visible {
 	-webkit-transform: translateX(0px);
 }

 .reveal .slides section .fragment.slide-in.left {
 	-webkit-transform: translateX(-1000px);
 }

 .reveal .slides section .fragment.slide-in.left.visible {
 	-webkit-transform: translateX(0px);
 }


 .reveal .slides section .fragment.slide-in.bottom {
 	-webkit-transform: translateY(1000px);
 }

 .reveal .slides section .fragment.slide-in.bottom.visible {
 	-webkit-transform: translateY(0px);
 }
 

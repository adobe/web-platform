	line-height: 0.9em;
	letter-spacing: 0.02em;
	color: $dark-color;
}

.reveal h1 { font-size: 80px; margin-top: 0.0em;}
.reveal h2 { font-size: 66px; 	margin-top: 0.0em; margin-bottom: 1em}
.reveal h3 { font-size: 56px;	}
.reveal h4 { font-size: 36px;	}


.reveal h2 {
   margin-bottom: 0.5em;
}

.reveal h1.inverted,
.reveal h2.inverted,
.reveal h3.inverted,
.reveal h4.inverted, 
span.inverted {
	color: rgba(255, 255, 255, 1);
    text-shadow: 2px 2px 0px rgba(0, 0, 0, 0.15), 0px -1px 0px #fff, 0em 0em 0.15em rgba(0, 0, 0, 0.5);
}

.reveal .subtitle {
    display: block;
    font-size: 0.75em;
    color: white;
    text-shadow: rgba(0, 0, 0, 0.8) 1px 1px 3px;
}



/*********************************************
 * SLIDES
 *********************************************/
.reveal .slides p {
 	/* position: absolute;
 	width: 1100px;
